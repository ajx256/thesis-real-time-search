    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB backupTestsAS",
            "type": "cppdbg",
            "request": "launch",
            "program": "/mnt/a/ComputerScience/Thesis/thesis-real-time-search/backupTestsAS.sh",
            "args": ["TreeWorld", "10", "<", "worlds/treeWorld/b2d10-1.tw"],
            "stopAtEntry": false,
            "cwd": "/mnt/a/ComputerScience/Thesis/thesis-real-time-search",
            "environment": [],
            "externalConsole": true,
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeProgram": "${env:windir}\\system32\\bash.exe",
                "pipeArgs": ["-c"],
                "pipeCwd": ""
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/mnt/a": "A:\\",
                "A:\\": "/mnt/a"
            }
        },
